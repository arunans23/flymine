#!/bin/bash

# The following environment variables need to be set
# FLYMINE: location of local flymine installation

cd $FLYMINE

CP=$CLASSPATH

for i in $FLYMINE/lib/*.jar ; do
    CP=$CP:$i
done

CP=$CP:build:build/test:build/java:build/model/testmodel:build/model/fulldata

java -cp $CP org.flymine.objectstore.SetupDataTestCase $*

