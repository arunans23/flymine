# ------------------ Define ProteinInteraction ---------------- #

# Reperesents an interaction between two proteins
:ProteinInteraction a owl:Class;
    rdfs:subClassOf :Relation;
    rdfs:subClassOf
      [ a owl:Restriction ;
        owl:maxCardinality "1" ;
        owl:onProperty :ProteinInteraction__bait ] ;
    rdfs:subClassOf
      [ a owl:Restriction ;
        owl:maxCardinality "1" ;
        owl:onProperty :ProteinInteraction__prey ] .

:ProteinInteraction__bait a owl:ObjectProperty;
    rdfs:domain :ProteinInteraction;
    rdfs:range :Protein.

:ProteinInteraction__prey a owl:ObjectProperty;
    rdfs:domain :ProteinInteraction;
    rdfs:range :Protein.


# ------------------- ExperimentType details -------------------- #

:ProteinInteractionExperiment a owl:Class;
    rdfs:subClassOf :Experiment;
    owl:equivalentClass psi:ExperimentType.

null:ExperimentType__identifier a owl:DatatypeProperty;
    owl:equivalentProperty psi:ExperimentType__identifier.

:ProteinInteractionExperiment__name a owl:DatatypeProperty;
    rdfs:domain :ProteinInteractionExperiment;
    rdfs:range xsd:string.

:ProteinInteractionExperiment__comments a owl:ObjectProperty;
    rdfs:domain :ProteinInteractionExperiment;
    rdfs:range :Comment.

null:ExperimentType__attributeLlist a owl:ObjectProperty;
    owl:equivalentProperty psi:ExperimentType__attributeList.

null:ExperimentType__names a owl:ObjectProperty;
    owl:equivalentProperty psi:ExperimentType__names.

null:ExperimentType__bibref a owl:ObjectProperty;
    owl:equivalentProperty psi:ExperimentType__bibref.

null:Confidence_ExperimentType a owl:Class;
    owl:equivalentClass psi:Confidence_ExperimentType.


:Publication a owl:Class ;
    rdfs:subClassOf psi:BibrefType ;
      rdfs:subClassOf
        [ a owl:Restriction ;
           owl:onProperty psi:BibrefType__xref ;
           owl:hasValue
               [  rdfs:subClassOf psi:XrefType ;
                  rdfs:subClassOf
                      [ a owl:Restriction ;
                        owl:onProperty psi:XRefType__primaryRef ;
                        owl:hasValue 
                          [  rdfs:subClassOf psi:DbReferenceType ;
                             rdfs:subClassOf
                               [ a owl:Restriction ;
                                 owl:onProperty psi:DbReferenceType__db ;
                                 owl:hasValue "pubmed"
                               ]
                          ]
                      ]
              ]
       ].


# ------------------- InteractorType ------------------- #

:Protein a owl:Class;
    rdfs:subClassOf
      [ a owl:Restriction ;
        owl:maxCardinality "1" ;
        owl:onProperty :Protein__sequence ] ;
    owl:equivalentClass psi:ProteinInteractorType.

# need to create Sequence object and set in code from psi:ProteinInteractorType__sequence.
:Protein__sequence a owl:ObjectProperty;
    rdfs:domain :Protein;
    rdfs:range :Sequence.

:Protein__organism a owl:ObjectProperty;
    rdfs:subPropertyOf :BioEntity__organism;
    rdfs:domain :Protein;
    rdfs:range :Organism;
    owl:equivalentProperty psi:ProteinInteractorType__organism.


null:ProteinInteractorType__identifier a owl:DatatypeProperty;
    owl:equivalentProperty psi:ProteinInteractorType__identifier.

null:ProteinInteractorType__sequence a owl:DatatypeProperty;
    owl:equivalentProperty psi:ProteinInteractorType__sequence.

null:ProteinInteractorType__names a owl:ObjectProperty;
    owl:equivalentProperty psi:ProteinInteractorType__names.

null:ProteinInteractorType__xref a owl:ObjectProperty;
    owl:equivalentProperty psi:ProteinInteractorType__xref.


