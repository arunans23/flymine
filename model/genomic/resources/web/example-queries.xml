<query-list>
  <query name="tutorial1" model="genomic" view="Gene.name"/>
  <query name="tutorial2" model="genomic" view="Gene.name Gene.organism.name">
    <node path="Gene" type="Gene"/>
    <node path="Gene.organism" type="Organism"/>
    <node path="Gene.organism.name" type="String">
      <constraint op="=" value="Drosophila melanogaster"/>
    </node>
  </query>
  <query name="tutorial3" model="genomic" view="Orthologue.object.name Orthologue.subject.name Orthologue.evidence.analysis">
    <node path="Orthologue" type="Orthologue"/>
    <node path="Orthologue.object" type="BioEntity"/>
    <node path="Orthologue.object.organism" type="Organism"/>
    <node path="Orthologue.object.organism.name" type="String">
      <constraint op="LIKE" value="%melanogaster"/>
    </node>
    <node path="Orthologue.subject" type="BioEntity"/>
    <node path="Orthologue.subject.organism" type="Organism"/>
    <node path="Orthologue.subject.organism.name" type="String">
      <constraint op="LIKE" value="%gambiae"/>
    </node>
    <node path="Orthologue.evidence" type="AnalysisResult"/>
  </query>
</query-list>
