<template-list>
  <template name="gene2ProteinInteraction2Structure" description="For the protein product of a particular gene, show any proteins it has been shown to interact with and for these proteins any regions which have a Pfam domain with a predicted 3-D structure." category="Proteomics">
    <query name="gene2ProteinInteraction2Structure" model="genomic" view="Synonym.subject.name Synonym.subject.proteins.primaryAccession Synonym.subject.proteins.interactions.bait.primaryAccession Synonym.subject.proteins.interactions.prey.primaryAccession Synonym.subject.proteins.interactions.bait.regions.sequenceFamily.name Synonym.subject.proteins.interactions.bait.regions.structures Synonym.subject.proteins.interactions.prey.regions.sequenceFamily.name Synonym.subject.proteins.interactions.prey.regions.structures">
      <node path="Synonym" type="Synonym">
      </node>
      <node path="Synonym.value" type="String">
        <constraint op="=" value="128up" description="Show protein interactions and Pfam domains with predicted 3-D structures for the protein product of gene:" identifier="c1" editable="true">
        </constraint>
      </node>
      <node path="Synonym.subject" type="Gene">
      </node>
    </query>
  </template>
  <template name="Object_Synonym" description="Find an entry in the database using any identifier or synonym." category="Genomics">
    <query name="Object_Synonym" model="genomic" view="Synonym.subject">
      <node path="Synonym" type="Synonym">
      </node>
      <node path="Synonym.value" type="String">
        <constraint op="=" value="timeout" description="" identifier="Synonym.value.1" editable="true">
        </constraint>
      </node>
    </query>
  </template>
  <template name="synonym2proteinInteractions1" description="For the protein product of a particular gene, show the proteins it has been shown to interact with, the confidence value assigned to this interaction, the interaction detection method and the publication for the experiment performed." category="Proteomics">
    <query name="synonym2proteinInteractions1" model="genomic" view="Synonym.subject.name Synonym.subject.organismDbId Synonym.subject.proteins.primaryAccession Synonym.subject.proteins.interactions.bait.primaryAccession Synonym.subject.proteins.interactions.prey.primaryAccession Synonym.subject.proteins.interactions.evidence.confidence Synonym.subject.proteins.interactions.evidence.analysis.interactionDetection.name Synonym.subject.proteins.interactions.evidence.analysis.publication.pubMedId">
      <node path="Synonym" type="Synonym">
      </node>
      <node path="Synonym.value" type="String">
        <constraint op="=" value="128up" description="Show the protein interactions for the protein product of gene:" identifier="c1" editable="true">
        </constraint>
      </node>
      <node path="Synonym.subject" type="Gene">
      </node>
      <node path="Synonym.subject.proteins" type="Protein">
      </node>
      <node path="Synonym.subject.proteins.interactions" type="ProteinInteraction">
      </node>
      <node path="Synonym.subject.proteins.interactions.evidence" type="ExperimentalResult">
      </node>
      <node path="Synonym.subject.proteins.interactions.evidence.analysis" type="ProteinInteractionExperiment">
      </node>
      <node path="Synonym.subject.organism" type="Organism">
      </node>
      <node path="Synonym.subject.organism.name" type="String">
        <constraint op="=" value="Drosophila melanogaster" description="from" identifier="c2" editable="true">
        </constraint>
      </node>
    </query>
  </template>
  <template name="Gene2ProteinStructure2.1" description="For a particular Drosophila gene show any regions of it's protein for which there is a predicted 3-D structure." category="Proteomics">
    <query name="Gene2ProteinStructure2.1" model="genomic" view="Synonym.subject.organismDbId Synonym.subject.name Synonym.subject.proteins.primaryAccession Synonym.subject.proteins.regions.sequenceFamily.name Synonym.subject.proteins.regions.objects.start Synonym.subject.proteins.regions.objects.end Synonym.subject.proteins.regions.structures">
      <node path="Synonym" type="Synonym">
      </node>
      <node path="Synonym.value" type="String">
        <constraint op="=" value="FBgn0025936" description="Show regions which have predicted 3-D structure for the protein of gene:" identifier="c1" editable="true">
        </constraint>
      </node>
      <node path="Synonym.subject" type="Gene">
      </node>
      <node path="Synonym.subject.proteins" type="Protein">
      </node>
      <node path="Synonym.subject.proteins.regions" type="ProteinRegion">
      </node>
      <node path="Synonym.subject.proteins.regions.objects" type="Location">
      </node>
      <node path="Synonym.subject.proteins.regions.objects.object" type="Protein">
      </node>
      <node path="Synonym.subject.organism" type="Organism">
      </node>
      <node path="Synonym.subject.organism.name" type="String">
        <constraint op="=" value="Drosophila melanogaster" description="from:" identifier="c2">
        </constraint>
      </node>
    </query>
  </template>
  <template name="GOidentifier2proteinInteractions1" description="Show protein interactions for proteins annotated with a particular GO identifier in a particular organism." category="Proteomics">
    <query name="GOidentifier2proteinInteractions1" model="genomic" view="GOTerm.identifier GOTerm.name GOTerm.genes.name GOTerm.genes.organismDbId GOTerm.genes.proteins.primaryAccession GOTerm.genes.proteins.interactions.bait.primaryAccession GOTerm.genes.proteins.interactions.prey.primaryAccession GOTerm.genes.proteins.interactions.evidence.analysis.interactionDetection.name GOTerm.genes.proteins.interactions.evidence.analysis.publication.pubMedId GOTerm.genes.organism.name">
      <node path="GOTerm" type="GOTerm">
      </node>
      <node path="GOTerm.identifier" type="String">
        <constraint op="=" value="GO:0007568" description="Show protein interactions for proteins annotated with GO identifier:" identifier="c1" editable="true">
        </constraint>
      </node>
      <node path="GOTerm.genes" type="Gene">
      </node>
      <node path="GOTerm.genes.proteins" type="Protein">
      </node>
      <node path="GOTerm.genes.proteins.interactions" type="ProteinInteraction">
      </node>
      <node path="GOTerm.genes.proteins.interactions.evidence" type="AnalysisResult">
      </node>
      <node path="GOTerm.genes.proteins.interactions.evidence.analysis" type="ProteinInteractionExperiment">
      </node>
      <node path="GOTerm.genes.organism" type="Organism">
      </node>
      <node path="GOTerm.genes.organism.name" type="String">
        <constraint op="=" value="Drosophila melanogaster" description="from:" identifier="c2" editable="true">
        </constraint>
      </node>
    </query>
  </template>
  <template name="synonym2proteinInteractionsWithGO1" description="For the protein product of a particular gene show which proteins it has been shown to interact with together with the GO terms that have been assigned to these, the confidence value assigned to this interaction, the interaction detection method and the pubmed id for the interaction experiment." category="Proteomics">
    <query name="synonym2proteinInteractionsWithGO1" model="genomic" view="Synonym.subject.name Synonym.subject.organismDbId Synonym.subject.proteins.primaryAccession Synonym.subject.proteins.interactions.bait.primaryAccession Synonym.subject.proteins.interactions.bait.genes.GOTerms.identifier Synonym.subject.proteins.interactions.bait.genes.GOTerms.name Synonym.subject.proteins.interactions.prey.primaryAccession Synonym.subject.proteins.interactions.prey.genes.GOTerms.identifier Synonym.subject.proteins.interactions.prey.genes.GOTerms.name Synonym.subject.proteins.interactions.evidence.confidence Synonym.subject.proteins.interactions.evidence.analysis.interactionDetection.name Synonym.subject.proteins.interactions.evidence.analysis.publication.pubMedId">
      <node path="Synonym" type="Synonym">
      </node>
      <node path="Synonym.value" type="String">
        <constraint op="=" value="128up" description="Show the protein interactions and GO terms for the protein product of gene:" identifier="c1" editable="true">
        </constraint>
      </node>
      <node path="Synonym.subject" type="Gene">
      </node>
      <node path="Synonym.subject.proteins" type="Protein">
      </node>
      <node path="Synonym.subject.proteins.interactions" type="ProteinInteraction">
      </node>
      <node path="Synonym.subject.proteins.interactions.evidence" type="ExperimentalResult">
      </node>
      <node path="Synonym.subject.proteins.interactions.evidence.analysis" type="ProteinInteractionExperiment">
      </node>
      <node path="Synonym.subject.organism" type="Organism">
      </node>
      <node path="Synonym.subject.organism.name" type="String">
        <constraint op="=" value="Drosophila melanogaster" description="from:" identifier="c2" editable="true">
        </constraint>
      </node>
    </query>
  </template>
  <template name="Gene2GO1" description="Show the GO annotations for a particular gene" category="Genomics">
    <query name="Gene2GO1" model="genomic" view="Synonym.subject.name Synonym.subject.organismDbId Synonym.subject.identifier Synonym.subject.GOTerms.name Synonym.subject.GOTerms.identifier">
      <node path="Synonym" type="Synonym">
      </node>
      <node path="Synonym.value" type="String">
        <constraint op="=" value="zen" description="Show the GO annotation for gene:" identifier="c1" editable="true">
        </constraint>
      </node>
      <node path="Synonym.subject" type="Gene">
      </node>
      <node path="Synonym.subject.organism" type="Organism">
      </node>
      <node path="Synonym.subject.organism.name" type="String">
        <constraint op="=" value="Drosophila melanogaster" description="from:" identifier="c2" editable="true">
        </constraint>
      </node>
    </query>
  </template>
  <template name="gene2ProteinStructure1" description="Show genes whose proteins have regions for which there is a predicted 3-D structure.  Give the location of the region on the protein and the structure details." category="Proteomics">
    <query name="gene2ProteinStructure1" model="genomic" view="Gene.name Gene.organismDbId Gene.proteins.primaryAccession Gene.proteins.regions.sequenceFamily.name Gene.proteins.regions.objects.start Gene.proteins.regions.objects.end Gene.proteins.regions.structures">
      <node path="Gene" type="Gene">
      </node>
      <node path="Gene.organism" type="Organism">
      </node>
      <node path="Gene.organism.name" type="String">
        <constraint op="=" value="Drosophila melanogaster" description="Show genes from:" identifier="c1" editable="true">
        </constraint>
      </node>
      <node path="Gene.proteins" type="Protein">
      </node>
      <node path="Gene.proteins.regions" type="ProteinRegion">
      </node>
      <node path="Gene.proteins.regions.objects" type="Location">
      </node>
      <node path="Gene.proteins.regions.objects.object" type="Protein">
      </node>
    </query>
  </template>
  <template name="chromosomeLocation2genes1" description="Show the genes, transcripts and exons located between two points on a chromosome.  Give the location of the exons." category="Genomics">
    <query name="chromosomeLocation2genes1" model="genomic" view="Chromosome.identifier Chromosome.exons.gene.name Chromosome.exons.gene.organismDbId Chromosome.exons.gene.identifier Chromosome.exons.transcripts.identifier Chromosome.exons.identifier Chromosome.exons.objects.start Chromosome.exons.objects.end Chromosome.exons.objects.strand">
      <node path="Chromosome" type="Chromosome">
      </node>
      <node path="Chromosome.identifier" type="String">
        <constraint op="=" value="2L" description="Show the genes, transcripts and exons located on chromosome:" identifier="c1" editable="true">
        </constraint>
      </node>
      <node path="Chromosome.organism" type="Organism">
      </node>
      <node path="Chromosome.organism.name" type="String">
        <constraint op="=" value="Drosophila melanogaster" description="from:" identifier="c2" editable="true">
        </constraint>
      </node>
      <node path="Chromosome.exons" type="Exon">
      </node>
      <node path="Chromosome.exons.objects" type="Location">
      </node>
      <node path="Chromosome.exons.objects.object" type="Chromosome">
      </node>
      <node path="Chromosome.exons.objects.start" type="Integer">
        <constraint op="&gt;" value="1" description="between base:" identifier="c3" editable="true">
        </constraint>
      </node>
      <node path="Chromosome.exons.objects.end" type="Integer">
        <constraint op="&lt;" value="10000" description="and:" identifier="c4" editable="true">
        </constraint>
      </node>
    </query>
  </template>
  <template name="Gene_Synonyms" description="For a particular genome database identifier, show all other identifiers and synonyms in FlyMine." category="Genomics">
    <query name="Gene_Synonyms" model="genomic" view="Gene.organismDbId Gene.synonyms.value">
      <node path="Gene" type="Gene">
      </node>
      <node path="Gene.organismDbId" type="String">
        <constraint op="=" value="FBgn0004053" description="" identifier="Gene.organismDbId.1" editable="true">
        </constraint>
      </node>
    </query>
  </template>
  <template name="synonymGene2exonChromosomeLocations2" description="Show the chromosome location of the exons for a particular gene" category="Genomics">
    <query name="synonymGene2exonChromosomeLocations2" model="genomic" view="Synonym.subject.name Synonym.subject.organismDbId Synonym.subject.chromosome.identifier Synonym.subject.exons.identifier Synonym.subject.exons.objects.start Synonym.subject.exons.objects.end">
      <node path="Synonym" type="Synonym">
      </node>
      <node path="Synonym.value" type="String">
        <constraint op="=" value="zen" description="Show the chromosome location of the exons of gene:" identifier="c1" editable="true">
        </constraint>
      </node>
      <node path="Synonym.subject" type="Gene">
      </node>
      <node path="Synonym.subject.exons" type="Exon">
      </node>
      <node path="Synonym.subject.exons.objects" type="Location">
      </node>
      <node path="Synonym.subject.exons.objects.object" type="Chromosome">
      </node>
      <node path="Synonym.subject.organism" type="Organism">
      </node>
      <node path="Synonym.subject.organism.name" type="String">
        <constraint op="=" value="Drosophila melanogaster" description="from organism:" identifier="c2" editable="true">
        </constraint>
      </node>
    </query>
  </template>
  <template name="orthologues1" description="Show the predicted orthologues between two organisms." category="Genomics">
    <query name="orthologues1" model="genomic" view="Orthologue.object Orthologue.subject">
      <node path="Orthologue" type="Orthologue">
      </node>
      <node path="Orthologue.object" type="BioEntity">
      </node>
      <node path="Orthologue.object.organism" type="Organism">
      </node>
      <node path="Orthologue.object.organism.name" type="String">
        <constraint op="=" value="Drosophila melanogaster" description="Show the predicted orthologues between:" identifier="c1" editable="true">
        </constraint>
      </node>
      <node path="Orthologue.subject" type="BioEntity">
      </node>
      <node path="Orthologue.subject.organism" type="Organism">
      </node>
      <node path="Orthologue.subject.organism.name" type="String">
        <constraint op="=" value="Anopheles gambiae" description="and:" identifier="c2" editable="true">
        </constraint>
      </node>
    </query>
  </template>
  <template name="DM_GoTerms" description="Show genes from Drosophila annotated with GO term." category="Genomics">
    <query name="DM_GoTerms" model="genomic" view="Gene">
      <node path="Gene" type="Gene">
      </node>
      <node path="Gene.organism" type="Organism">
      </node>
      <node path="Gene.organism.name" type="String">
        <constraint op="LIKE" value="%melanogaster">
        </constraint>
      </node>
      <node path="Gene.annotations" type="Annotation">
      </node>
      <node path="Gene.annotations.property" type="GOTerm">
      </node>
      <node path="Gene.annotations.property.identifier" type="String">
        <constraint op="=" value="GO:0007568" description="" identifier="Gene.annotations.property.identifier.1" editable="true">
        </constraint>
      </node>
    </query>
  </template>
  <template name="gene2Transcripts1" description="Show the predicted transcripts and the number of exons for a particular gene" category="Genomics">
    <query name="gene2Transcripts1" model="genomic" view="Synonym.subject Synonym.subject.transcripts Synonym.subject.transcripts.exonCount">
      <node path="Synonym" type="Synonym">
      </node>
      <node path="Synonym.value" type="String">
        <constraint op="=" value="zen" description="Show the predicted transcripts and the number of exons for gene:" identifier="c1" editable="true">
        </constraint>
      </node>
      <node path="Synonym.subject" type="Gene">
      </node>
      <node path="Synonym.subject.organism" type="Organism">
      </node>
      <node path="Synonym.subject.organism.name" type="String">
        <constraint op="=" value="Drosophila melanogaster" description="from:" identifier="c2" editable="true">
        </constraint>
      </node>
    </query>
  </template>
  <template name="Pfam2proteinStructure1" description="Show the Drosophila proteins which contain a particular Pfam domain for which there is also a predicted 3-D structure." category="Proteomics">
    <query name="Pfam2proteinStructure1" model="genomic" view="ProteinSequenceFamily.name ProteinSequenceFamily.regions.objects.object.primaryAccession ProteinSequenceFamily.regions.objects.start ProteinSequenceFamily.regions.objects.end ProteinSequenceFamily.regions.structures">
      <node path="ProteinSequenceFamily" type="ProteinSequenceFamily">
      </node>
      <node path="ProteinSequenceFamily.name" type="String">
        <constraint op="=" value="PF00812" description="Show the Drosophila proteins which contain Pfam domain:  (and which also have a predicted 3-D structure for this region.)" identifier="c1" editable="true">
        </constraint>
      </node>
      <node path="ProteinSequenceFamily.regions" type="ProteinRegion">
      </node>
      <node path="ProteinSequenceFamily.regions.objects" type="Location">
      </node>
      <node path="ProteinSequenceFamily.regions.objects.object" type="Protein">
      </node>
    </query>
  </template>
  <template name="Genes2chromosome1" description="Show genes located on a particular chromsome." category="Genomics">
    <query name="Genes2chromosome1" model="genomic" view="Gene.name Gene.organismDbId Gene.chromosome.identifier">
      <node path="Gene" type="Gene">
      </node>
      <node path="Gene.chromosome" type="Chromosome">
      </node>
      <node path="Gene.chromosome.identifier" type="String">
        <constraint op="=" value="2L" description="Show genes located on chromosome" identifier="c1" editable="true">
        </constraint>
      </node>
      <node path="Gene.organism" type="Organism">
      </node>
      <node path="Gene.organism.name" type="String">
        <constraint op="=" value="Drosophila melanogaster" description="from" identifier="c2" editable="true">
        </constraint>
      </node>
    </query>
  </template>
</template-list>
