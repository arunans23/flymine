<webconfig>
  <class name="org.flymine.model.genomic.Database">
    <shortdisplayers>
      <displayer src="/model/database.jsp"/>
      <displayer src="/objectFields.jsp"/>
    </shortdisplayers>
    <longdisplayers>
      <displayer src="/model/database.jsp"/>
      <displayer src="/objectFields.jsp"/>
    </longdisplayers>
  </class>
  <class name="org.flymine.model.genomic.Organism">
    <shortdisplayers>
      <displayer src="/model/organism.jsp"/>
      <displayer src="/objectFields.jsp"/>
    </shortdisplayers>
    <longdisplayers>
      <displayer src="/model/organism.jsp"/>
      <displayer src="/objectFields.jsp"/>
    </longdisplayers>
  </class>
  <class name="org.flymine.model.genomic.Gene">
    <shortdisplayers>
      <displayer src="/model/geneShort.jsp"/>
      <displayer src="/objectFields.jsp"/>
    </shortdisplayers>
    <longdisplayers>
      <displayer src="/model/geneLong.jsp"/>
      <displayer src="/objectFields.jsp"/>
    </longdisplayers>
  </class>

  <exporter id="sequenceExporter" actionPath="/exportAction?type=sequenceExporter"
            className="org.flymine.web.SequenceExporter"/>
</webconfig>
