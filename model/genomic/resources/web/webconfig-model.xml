<webconfig>
  <class className="org.flymine.model.genomic.ModelledProteinStructure">
    <longdisplayers>
      <displayer src="/model/modelledStructure.jsp"/>
    </longdisplayers>
  </class>
  <class className="org.flymine.model.genomic.Database">
    <shortdisplayers>
      <displayer src="/model/databaseShort.jsp"/>
    </shortdisplayers>
    <longdisplayers>
      <displayer src="/model/databaseLong.jsp"/>
    </longdisplayers>
  </class>
  <class className="org.flymine.model.genomic.Organism">
    <shortdisplayers>
      <displayer src="/model/organismShort.jsp"/>
    </shortdisplayers>
    <longdisplayers>
      <displayer src="/model/organismLong.jsp"/>
    </longdisplayers>
  </class>
  <class className="org.flymine.model.genomic.LocatedSequenceFeature" fieldName="residues">
    <longdisplayers>
      <displayer src="/hiddenField.jsp"/>
    </longdisplayers>
  </class>
  <class className="org.flymine.model.genomic.LocatedSequenceFeature">
    <longdisplayers>
      <displayer src="/model/locatedSequenceFeatureImage.jsp"/>
    </longdisplayers>
  </class>
  <class className="org.flymine.model.genomic.Exon">
    <longdisplayers>
      <displayer src="/model/locatedSequenceFeatureImage.jsp"/>
    </longdisplayers>
  </class>
  <class className="org.flymine.model.genomic.Gene">
    <longdisplayers>
      <displayer src="/model/geneLong.jsp"/>
      <displayer src="/model/locatedSequenceFeatureImage.jsp"/>
    </longdisplayers>
  </class>

  <exporter id="sequenceExporter" actionPath="/exportAction?type=sequenceExporter"
            className="org.flymine.web.SequenceExporter"/>
</webconfig>
