<webconfig>
  <class name="org.flymine.model.genomic.Database">
    <shortdisplayers>
      <displayer src="/model/database.jsp"/>
    </shortdisplayers>
    <longdisplayers>
      <displayer src="/model/database.jsp"/>
    </longdisplayers>
  </class>
  <class name="org.flymine.model.genomic.Organism">
    <shortdisplayers>
      <displayer src="/model/organism.jsp"/>
    </shortdisplayers>
    <longdisplayers>
      <displayer src="/model/organism.jsp"/>
    </longdisplayers>
  </class>
  <class name="org.flymine.model.genomic.Gene">
    <shortdisplayers>
      <displayer src="/oneField.jsp">
        <param name="field" value="uniquename"/>
      </displayer>
      <displayer src="/oneField.jsp">
        <param name="field" value="name"/>
      </displayer>
    </shortdisplayers>
    <longdisplayers>
      <displayer src="/oneField.jsp">
        <param name="field" value="uniquename"/>
      </displayer>
      <displayer src="/oneField.jsp">
        <param name="field" value="name"/>
      </displayer>
      <displayer src="/oneField.jsp">
        <param name="field" value="seqlen"/>
      </displayer>
    </longdisplayers>
  </class>
</webconfig>
