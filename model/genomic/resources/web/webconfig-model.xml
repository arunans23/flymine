<webconfig>
  
  <class className="org.flymine.model.genomic.Disease">
    <fields>
      <fieldconfig fieldExpr="omimId"/>
      <fieldconfig fieldExpr="description"/>
    </fields>
    <longdisplayers>
      <displayer src="/model/disease.jsp"/>
    </longdisplayers>
  </class>
  
  <class className="org.flymine.model.genomic.ModelledProteinStructure">
    <fields>
      <fieldconfig fieldExpr="ZScore" showInSummary="true" showInInlineCollection="true" showInResults="true"/>
      <fieldconfig fieldExpr="QScore" showInSummary="true" showInInlineCollection="true" showInResults="true"/>
    </fields>
    <longdisplayers>
      <displayer src="/model/modelledStructure.jsp"/>
    </longdisplayers>
  </class>

  <class className="org.flymine.model.genomic.Publication">
    <fields>
      <fieldconfig fieldExpr="firstAuthor" showInSummary="true" showInInlineCollection="true" showInResults="true"/>
      <fieldconfig fieldExpr="title" showInSummary="true" showInInlineCollection="true" showInResults="true"/>
    </fields>

    <longdisplayers>
      <displayer src="/model/publication.jsp"/>
    </longdisplayers>
  </class>

  <class className="org.flymine.model.genomic.Author">
    <fields>
      <fieldconfig fieldExpr="name" showInSummary="true" showInInlineCollection="true" showInResults="true"/>
    </fields>
  </class>

  <class className="org.flymine.model.genomic.DataSource">
    <longdisplayers>
      <displayer src="/model/dataSourceLong.jsp"/>
    </longdisplayers>
    <tabledisplayer src="/model/dataSourceShort.jsp"/>
  </class>

  <class className="org.flymine.model.genomic.Organism">
    <fields>
      <fieldconfig fieldExpr="name" showInSummary="true" showInInlineCollection="true" showInResults="true"/>
      <fieldconfig fieldExpr="taxonId" showInSummary="true" showInInlineCollection="true" showInResults="true"/>
      <fieldconfig fieldExpr="abbreviation" showInSummary="true" showInInlineCollection="true" showInResults="true"/>
    </fields>
    <longdisplayers>
      <displayer src="/model/organismLong.jsp"/>
    </longdisplayers>
  </class>

  <class className="org.flymine.model.genomic.LocatedSequenceFeature">
    <fields>
      <fieldconfig fieldExpr="identifier" showInSummary="true" showInInlineCollection="true" showInResults="true"/>
    </fields>
    <longdisplayers>
      <displayer src="/model/locatedSequenceFeatureImage.jsp"/>
    </longdisplayers>
  </class>

  <class className="org.flymine.model.genomic.Exon">
    <fields>
      <fieldconfig fieldExpr="identifier" showInSummary="true" showInInlineCollection="true" showInResults="true"/>
      <fieldconfig fieldExpr="gene.identifier" showInSummary="true" showInInlineCollection="true" showInResults="true"/>
    </fields>
  </class>

  <class className="org.flymine.model.genomic.Gene">
    <fields>
      <fieldconfig fieldExpr="identifier" showInSummary="true" showInInlineCollection="true" showInResults="true"/>
      <fieldconfig fieldExpr="organismDbId" showInSummary="true" showInInlineCollection="true" showInResults="true"/>
      <fieldconfig fieldExpr="symbol" showInSummary="true" showInInlineCollection="true" showInResults="true"/>
    </fields>
    <longdisplayers>
      <displayer src="/model/synonymDisplayer.jsp"/>
      <displayer src="/model/locatedSequenceFeatureImage.jsp"/>
    </longdisplayers>
  </class>

  <class className="org.flymine.model.genomic.DefinedRegion">
    <fields>
      <fieldconfig fieldExpr="identifier" showInSummary="true" showInInlineCollection="true" showInResults="true"/>
      <fieldconfig fieldExpr="type" showInSummary="true" showInInlineCollection="true" showInResults="true"/>
    </fields>
    <longdisplayers>
      <displayer src="/model/synonymDisplayer.jsp"/>
      <displayer src="/model/locatedSequenceFeatureImage.jsp"/>
    </longdisplayers>
  </class>

  <class className="org.flymine.model.genomic.Chromosome">
    <fields>
      <fieldconfig fieldExpr="identifier" showInSummary="true" showInInlineCollection="true" showInResults="true"/>
      <fieldconfig fieldExpr="organism.shortName" showInSummary="true" showInInlineCollection="true" showInResults="true"/>
    </fields>
    <longdisplayers>
      <displayer src="/model/locatedSequenceFeatureImage.jsp"/>
    </longdisplayers>
  </class>

  <class className="org.flymine.model.genomic.BioEntity">
    <fields>
      <fieldconfig fieldExpr="identifier" showInSummary="true" showInInlineCollection="true" showInResults="true"/>
    </fields>
  </class>

  <class className="org.flymine.model.genomic.Protein">
    <fields>
      <fieldconfig fieldExpr="primaryAccession" showInSummary="true" showInInlineCollection="true" showInResults="true"/>
      <fieldconfig fieldExpr="identifier" showInSummary="true" showInInlineCollection="true" showInResults="true"/>
      <fieldconfig fieldExpr="organism.shortName" showInSummary="true" showInInlineCollection="true" showInResults="true"/>
    </fields>
    <longdisplayers>
      <displayer src="/model/synonymDisplayer.jsp"/>
    </longdisplayers>
  </class>

  <class className="org.flymine.model.genomic.ProteinSequenceFamily">
    <fields>
      <fieldconfig fieldExpr="name" showInSummary="true" showInInlineCollection="true" showInResults="true"/>
    </fields>
    <longdisplayers>
      <displayer src="/model/pfamNameLink.jsp"/>
    </longdisplayers>
  </class>

  <class className="org.flymine.model.genomic.Transcript">
    <fields>
      <fieldconfig fieldExpr="identifier" showInSummary="true" showInInlineCollection="true" showInResults="true"/>
      <fieldconfig fieldExpr="organism.shortName" showInSummary="true" showInInlineCollection="true" showInResults="true"/>
      <fieldconfig fieldExpr="exonCount" showInSummary="true" showInInlineCollection="true" showInResults="true"/>
    </fields>
  </class>

  <class className="org.flymine.model.genomic.MicroarrayOligo">
    <longdisplayers>
      <!-- disable the LocatedSequenceFeature displayer (GBrowse) because
           MicroarrayOligos aren't located at the moment -->
    </longdisplayers>
  </class>

  <class className="org.flymine.model.genomic.Relation">
    <fields>
      <fieldconfig fieldExpr="object.identifier" showInSummary="true" showInInlineCollection="true" showInResults="true"/>
      <fieldconfig fieldExpr="subject.identifier" showInSummary="true" showInInlineCollection="true" showInResults="true"/>
    </fields>
  </class>

  <class className="org.flymine.model.genomic.Orthologue">
    <fields>
      <fieldconfig fieldExpr="object.identifier" showInSummary="true" showInInlineCollection="true" showInResults="true"/>
      <fieldconfig fieldExpr="object.organism.shortName" showInSummary="true" showInInlineCollection="true" showInResults="true"/>
      <fieldconfig fieldExpr="subject.identifier" showInSummary="true" showInInlineCollection="true" showInResults="true"/>
      <fieldconfig fieldExpr="subject.organism.shortName" showInSummary="true" showInInlineCollection="true" showInResults="true"/>
    </fields>
  </class>

  <class className="org.flymine.model.genomic.ProteinInteraction">
    <fields>
      <fieldconfig fieldExpr="bait.primaryAccession" showInSummary="true" showInInlineCollection="true" showInResults="true"/>
      <fieldconfig fieldExpr="prey.primaryAccession" showInSummary="true" showInInlineCollection="true" showInResults="true"/>
    </fields>
  </class>

  <class className="org.flymine.model.genomic.Location">
    <fields>
      <fieldconfig fieldExpr="object.identifier" showInSummary="true" showInInlineCollection="true" showInResults="true"/>
      <fieldconfig fieldExpr="subject.identifier" showInSummary="true" showInInlineCollection="true" showInResults="true"/>
      <fieldconfig fieldExpr="start" showInSummary="true" showInInlineCollection="true" showInResults="true"/>
      <fieldconfig fieldExpr="end" showInSummary="true" showInInlineCollection="true" showInResults="true"/>
    </fields>
  </class>

  <class className="org.flymine.model.genomic.InfoSource">
    <fields>
      <fieldconfig fieldExpr="title" showInSummary="true" showInInlineCollection="true" showInResults="true"/>
    </fields>
  </class>

  <class className="org.flymine.model.genomic.InterproEvidence">
    <fields>
      <fieldconfig fieldExpr="infotext" showInSummary="true" showInInlineCollection="true" showInResults="true"/>
    </fields>
  </class>

  <class className="org.flymine.model.genomic.AnalysisResult">
    <fields>
      <fieldconfig fieldExpr="confidence" showInSummary="true" showInInlineCollection="true" showInResults="true"/>
      <fieldconfig fieldExpr="source.title" showInSummary="true" showInInlineCollection="true" showInResults="true"/>
    </fields>
  </class>

  <class className="org.flymine.model.genomic.ComputationalResult">
    <fields>
      <fieldconfig fieldExpr="score" showInSummary="true" showInInlineCollection="true" showInResults="true"/>
      <fieldconfig fieldExpr="confidence" showInSummary="true" showInInlineCollection="true" showInResults="true"/>
      <fieldconfig fieldExpr="source.title" showInSummary="true" showInInlineCollection="true" showInResults="true"/>
    </fields>
  </class>

  <class className="org.flymine.model.genomic.OntologyTerm">
    <fields>
      <fieldconfig fieldExpr="identifier" showInSummary="true" showInInlineCollection="true" showInResults="true"/>
      <fieldconfig fieldExpr="name" showInSummary="true" showInInlineCollection="true" showInResults="true"/>
      <fieldconfig fieldExpr="description" doNotTruncate="true" />
    </fields>
  </class>

  <class className="org.flymine.model.genomic.OntologyRelation">
    <fields>
      <fieldconfig fieldExpr="type" showInSummary="true" showInInlineCollection="true" showInResults="true"/>
      <fieldconfig fieldExpr="parentTerm.identifier" showInSummary="true" showInInlineCollection="true" showInResults="true"/>
      <fieldconfig fieldExpr="childTerm.identifier" showInSummary="true" showInInlineCollection="true" showInResults="true"/>
    </fields>
  </class>
  
  <class className="org.flymine.model.genomic.OntologyTermSynonym">
    <fields>
      <fieldconfig fieldExpr="name" showInSummary="true" showInInlineCollection="true" showInResults="true"/>
      <fieldconfig fieldExpr="type" showInSummary="true" showInInlineCollection="true" showInResults="true"/>
    </fields>
  </class>
  
  <class className="org.flymine.model.genomic.MicroArrayExperiment">
    <fields>
      <fieldconfig fieldExpr="name" showInSummary="true" showInInlineCollection="true" showInResults="true"/>
      <fieldconfig fieldExpr="description" doNotTruncate="true"/>
    </fields>
  </class>

  <class className="org.flymine.model.genomic.Annotation">
    <fields>
      <fieldconfig fieldExpr="subject.identifier" showInSummary="true" showInInlineCollection="true" showInResults="true"/>
    </fields>
  </class>

  <class className="org.flymine.model.genomic.Synonym">
    <fields>
      <fieldconfig fieldExpr="value" showInSummary="true" showInInlineCollection="true" showInResults="true"/>
      <fieldconfig fieldExpr="type" showInSummary="true" showInInlineCollection="true" showInResults="true"/>
      <fieldconfig fieldExpr="source.title" showInSummary="true" showInInlineCollection="true" showInResults="true"/>
    </fields>
  </class>

  <class className="org.flymine.model.genomic.Sequence">
    <fields>
      <fieldconfig fieldExpr="residues" fieldExporter="org.flymine.web.ResidueFieldExporter"
                   sectionOnRight="true" sectionTitle="Sequence" openByDefault="true"/>
    </fields>
  </class>

  <class className="org.flymine.model.genomic.MicroArrayResult">
    <fields>
      <fieldconfig fieldExpr="type" showInSummary="true" showInInlineCollection="true" showInResults="true"/>
      <fieldconfig fieldExpr="value" showInSummary="true" showInInlineCollection="true" showInResults="true"/>
      <fieldconfig fieldExpr="scale" showInSummary="true" showInInlineCollection="true" showInResults="true"/>
      <fieldconfig fieldExpr="isControl" showInSummary="true" showInInlineCollection="true" showInResults="true"/>
    </fields>
  </class>

  <class className="org.flymine.model.genomic.Reporter">
    <fields>
      <fieldconfig fieldExpr="material.identifier" showInSummary="true" showInInlineCollection="true"/>
      <fieldconfig fieldExpr="failType" showInSummary="true" showInInlineCollection="true" showInResults="true"/>
      <fieldconfig fieldExpr="isControl" showInSummary="true" showInInlineCollection="true" showInResults="true"/>
      <fieldconfig fieldExpr="controlType" showInSummary="true" showInInlineCollection="true" showInResults="true"/>
    </fields>
  </class>

  <class className="org.flymine.model.genomic.MicroArrayExperiment">
    <fields>
      <fieldconfig fieldExpr="name" showInSummary="true" showInInlineCollection="true" showInResults="true"/>
      <fieldconfig fieldExpr="description" sectionOnRight="true" sectionTitle="Description" openByDefault="true"/>
    </fields>
  </class>

  <class className="org.flymine.model.genomic.Sample">
    <fields>
      <fieldconfig fieldExpr="materialType" showInSummary="true" showInInlineCollection="true" showInResults="true"/>
      <fieldconfig fieldExpr="organism.shortName" showInSummary="true" showInInlineCollection="true" showInResults="true"/>
      <fieldconfig fieldExpr="primaryCharacteristicType" showInSummary="true" showInInlineCollection="true" showInResults="true"/>
      <fieldconfig fieldExpr="primaryCharacteristic" showInSummary="true" showInInlineCollection="true" showInResults="true"/>
    </fields>
  </class>

  <class className="org.flymine.model.genomic.SampleCharacteristic">
    <fields>
      <fieldconfig fieldExpr="type" showInSummary="true" showInInlineCollection="true" showInResults="true"/>
      <fieldconfig fieldExpr="value" showInSummary="true" showInInlineCollection="true" showInResults="true"/>
    </fields>
  </class>

  <class className="org.flymine.model.genomic.TreatmentParameter">
    <fields>
      <fieldconfig fieldExpr="type" showInSummary="true" showInInlineCollection="true" showInResults="true"/>
      <fieldconfig fieldExpr="value" showInSummary="true" showInInlineCollection="true" showInResults="true"/>
      <fieldconfig fieldExpr="units" showInSummary="true" showInInlineCollection="true" showInResults="true"/>
    </fields>
  </class>

  <class className="org.flymine.model.genomic.Treatment">
    <fields>
      <fieldconfig fieldExpr="action" showInSummary="true" showInInlineCollection="true" showInResults="true"/>
      <fieldconfig fieldExpr="protocol.name" showInInlineCollection="true"/>
    </fields>
  </class>

  <class className="org.flymine.model.genomic.Protocol">
    <fields>
      <fieldconfig fieldExpr="name" showInSummary="true" showInInlineCollection="true" showInResults="true"/>
      <fieldconfig fieldExpr="description" doNotTruncate="true"/>
    </fields>
  </class>

  <class className="org.flymine.model.genomic.MicroArrayAssay">
    <fields>
      <fieldconfig fieldExpr="sample1" showInSummary="true" showInInlineCollection="true" showInResults="true"/>
      <fieldconfig fieldExpr="sample2" showInSummary="true" showInInlineCollection="true" showInResults="true"/>
    </fields>
  </class>

  <class className="org.flymine.model.genomic.GOAnnotation">
    <fields>
      <fieldconfig fieldExpr="subject.identifier" showInSummary="true" showInInlineCollection="false" showInResults="true"/>
      <fieldconfig fieldExpr="identifier" showInSummary="true" showInInlineCollection="true" showInResults="true"/>
      <fieldconfig fieldExpr="name" showInSummary="true" showInInlineCollection="true" showInResults="true"/>
      <fieldconfig fieldExpr="evidenceCode" showInSummary="true" showInInlineCollection="true" showInResults="true"/>
    </fields>
  </class>


  <tableExportConfig id="sequenceExporter" actionPath="/exportAction?type=sequenceExporter"
                     className="org.flymine.web.SequenceExporter"/>
</webconfig>
