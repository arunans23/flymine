Chromosome = chromosome
Chromosome.identifier = chromosome.name
Chromosome.length = chromosome.length
ChromosomeBand = karyotype
ChromosomeBand.chromosome = karyotype.chromosome
ChromosomeBand.identifier = karyotype.band
ChromosomeBand.stain = karyotype.stain
ComputationalAnalysis = analysis
ComputationalAnalysis.algorithm = analysis.logic_name
Contig = contig
Contig.identifier = contig.name
Contig.length = contig.length
Contig.sequence = contig.dna
Exon = exon
Gene = gene
Protein = translation
Protein.endExon = translation.end_exon
Protein.startExon = translation.start_exon
Protein.translationEnd = translation.seq_end
Protein.translationStart = translation.seq_start
RankedRelation = exon_transcript
RankedRelation.object = exon_transcript.transcript
RankedRelation.rank = exon_transcript.rank
RankedRelation.subject = exon_transcript.exon
RepeatRegion = repeat_feature
RepeatRegion.contig = repeat_feature.contig
RepeatRegion.end = repeat_feature.repeat_end
RepeatRegion.start = repeat_feature.repeat_start
Sequence = dna
Sequence.residues = dna.sequence
Synonym = exon_stable_id, transcript_stable_id, gene_stable_id
Synonym.subject = gene_stable_id.gene, transcript_stable_id.transcript, exon_stable_id.exon
Synonym.value = transcript_stable_id.stable_id, exon_stable_id.stable_id, gene_stable_id.stable_id
Transcript = transcript
Transcript.exonCount = transcript.exon_count
Transcript.gene = transcript.gene
Transcript.protein = transcript.translation
TRNA = simple_feature [analysis.program = tRNAscan-SE]
