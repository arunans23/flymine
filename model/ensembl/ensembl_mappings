Chromosome = chromosome
Chromosome.identifier = chromosome.name
Chromosome.length = chromosome.length
ChromosomeBand = karyotype
ChromosomeBand.chromosome = karyotype.chromosome
ChromosomeBand.identifier = karyotype.band
ChromosomeBand.stain = karyotype.stain
ComputationalAnalysis = analysis
ComputationalAnalysis.algorithm = analysis.logic_name
Contig = contig
Contig.identifier = contig.name
Contig.length = contig.length
Contig.sequence = contig.dna
Exon = exon
Gene = gene
Translation = translation
Translation.endExon = translation.end_exon
Translation.startExon = translation.start_exon
Translation.translationEnd = translation.seq_end
Translation.translationStart = translation.seq_start
RankedRelation = exon_transcript
RankedRelation.object = exon_transcript.transcript
RankedRelation.rank = exon_transcript.rank
RankedRelation.subject = exon_transcript.exon
RepeatRegion = repeat_feature  [analysis.logic_name = TRF]
RepeatRegion.contig = repeat_feature.contig
Sequence = dna
Sequence.residues = dna.sequence
Synonym = exon_stable_id, transcript_stable_id, gene_stable_id, translation_stable_id
Synonym.subject = gene_stable_id.gene, transcript_stable_id.transcript, exon_stable_id.exon, translation_stable_id.translation
Synonym.value = transcript_stable_id.stable_id, exon_stable_id.stable_id, gene_stable_id.stable_id, translation_stable_id.stable_id
MRNA = transcript
MRNA.exonCount = transcript.exon_count
MRNA.gene = transcript.gene
MRNA.translation = transcript.translation
TRNA = simple_feature [analysis.program = tRNAscan-SE]
TRNA.contig = simple_feature.contig

