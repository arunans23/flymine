# Base: 
@prefix :        <http://www.flymine.org/model/genomic#> .
@prefix so:      <http://www.flymine.org/model/so#> .
@prefix null:    <http://www.flymine.org/model/null#> .
@prefix rdfs:    <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf:     <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .
@prefix xsd:     <http://www.w3.org/2001/XMLSchema#> .


:BioEntity a owl:Class ;
    owl:equivalentClass so:SequenceOntology, so:So.

# replace Transcript__genes collection with Transcript_gene reference
null:Transcript__genes a owl:ObjectProperty;
    owl:equivalentProperty so:Transcript__genes.

:Transcript__gene a owl:ObjectProperty;
    rdfs:domain :Transcript;
    rdfs:range :Gene.

:Transcript a owl:Class;
    rdfs:subClassOf 
      [ a owl:Restriction ;
        owl:maxCardinality "1" ;
        owl:onProperty :Transcript__gene ] .

#define a Sequence object and reference from LocatedSequenceFeature

:Sequence a owl:Class.

:Sequence__sequence a owl:DatatypeProperty;
    rdfs:domain :Sequence;
    rdfs:range xsd:string.

:LocatedSequenceFeature a owl:Class;
    rdfs:subClassOf
      [ a owl:Restriction ;
        owl:maxCardinality "1" ;
        owl:onProperty :LocatedSequenceFeature__sequence ] .

:LocatedSequenceFeature__sequence a owl:ObjectProperty;
    rdfs:domain :LocatedSequenceFeature;
    rdfs:range :Sequence.

# ChromsomeBand not in SO so not a LocatedSequenceFeature
:ChromosomeBand a owl:Class;
    rdfs:subClassOf
      [ a owl:Restriction ;
        owl:maxCardinality "1" ;
        owl:onProperty :ChromosomeBand__sequence ] .

:ChromosomeBand__sequence a owl:ObjectProperty;
    rdfs:domain :ChromosomeBand;
    rdfs:range :Sequence.
