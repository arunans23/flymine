@prefix :     <http://www.flymine.org/model/genomic#>.
@prefix phenotype: <http://www.flymine.org/model/phenotype#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix owl:  <http://www.w3.org/2002/07/owl#>.
@prefix xsd:  <http://www.w3.org/2001/XMLSchema#>.
@prefix null:	 <http://www.flymine.org/model/null#>.

:Phenotype a owl:Class;
    rdfs:subClassOf :BioProperty.

phenotype:Phenotype__ID a owl:DatatypeProperty ;
    rdfs:domain phenotype:Phenotype ;
    rdfs:range xsd:string .

phenotype:Phenotype__label a owl:DatatypeProperty ;
    rdfs:domain phenotype:Phenotype ;
    rdfs:range xsd:string .

:Phenotype__flyBaseId a owl:DatatypeProperty ;
    rdfs:domain :Phenotype ;
    rdfs:range xsd:string ;
    owl:equivalentProperty phenotype:Phenotype__ID .

:Phenotype__label a owl:DatatypeProperty ;
    rdfs:domain :Phenotype ;
    rdfs:range xsd:string ;
    owl:equivalentProperty phenotype:Phenotype__label .

:PhenotypeQualifier a owl:Class;
    rdfs:subClassOf :BioProperty.

phenotype:PhenotypeQualifier__ID a owl:DatatypeProperty ;
    rdfs:domain phenotype:PhenotypeQualifier ;
    rdfs:range xsd:string .

phenotype:PhenotypeQualifier__label a owl:DatatypeProperty ;
    rdfs:domain phenotype:PhenotypeQualifier ;
    rdfs:range xsd:string .

:PhenotypeQualifier__flyBaseId a owl:DatatypeProperty ;
    rdfs:domain :PhenotypeQualifier ;
    rdfs:range xsd:string ;
    owl:equivalentProperty phenotype:PhenotypeQualifier__ID .

:PhenotypeQualifier__label a owl:DatatypeProperty ;
    rdfs:domain :PhenotypeQualifier ;
    rdfs:range xsd:string ;
    owl:equivalentProperty phenotype:PhenotypeQualifier__label .
