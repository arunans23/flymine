@prefix :        <http://www.flymine.org/model/genomic#> .
@prefix rdfs:    <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf:     <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .
@prefix xsd:     <http://www.w3.org/2001/XMLSchema#> .

:Protein a owl:Class;
    rdfs:subClassOf :BioEntity.

:Protein__primaryAccession a owl:DatatypeProperty;
    rdfs:domain :Protein;
    rdfs:range xsd:string.

:Orthologue a owl:Class;
    rdfs:subClassOf :Relation;
    rdfs:subClassOf
      [ a owl:Restriction ;
        owl:maxCardinality "1" ;
        owl:onProperty :Orthologue__objectProtein ] ;
    rdfs:subClassOf
      [ a owl:Restriction ;
        owl:maxCardinality "1" ;
        owl:onProperty :Orthologue__subjectProtein ] .

:Orthologue__objectProtein a owl:ObjectProperty ;
    rdfs:domain :Orthologue ;
    rdfs:range :Protein .

:Orthologue__subjectProtein a owl:ObjectProperty ;
    rdfs:domain :Orthologue ;
    rdfs:range :Protein .

:Paralogue a owl:Class;
    rdfs:subClassOf :Relation;
    rdfs:subClassOf
      [ a owl:Restriction ;
        owl:maxCardinality "1" ;
        owl:onProperty :Paralogue__objectProtein ] ;
    rdfs:subClassOf
      [ a owl:Restriction ;
        owl:maxCardinality "1" ;
        owl:onProperty :Paralogue__subjectProtein ] .

:Paralogue__objectProtein a owl:ObjectProperty ;
    rdfs:domain :Paralogue ;
    rdfs:range :Protein .

:Paralogue__subjectProtein a owl:ObjectProperty ;
    rdfs:domain :Paralogue ;
    rdfs:range :Protein .

