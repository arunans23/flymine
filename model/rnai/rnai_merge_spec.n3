    rdfs:subClassOf :Embryonic .

:Early a owl:Class ;
    rdfs:subClassOf :PhenotypeQualifier .

:EarlyEmbryonic a owl:Class ;
    rdfs:subClassOf :Early ;
    rdfs:subClassOf :Embryonic .

:AbnormalSpindle a owl:Class ;
    rdfs:subClassOf :Phenotype .

:AbnormalSpindlePositioning a owl:Class ;
    rdfs:subClassOf :AbnormalSpindle .

:Adult a owl:Class ;
    rdfs:subClassOf :Adult .

:Lethal a owl:Class ;
