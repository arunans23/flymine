    rdfs:subClassOf :EarlyEmbryonic .

:CytokinesisDefective a owl:Class ;
    rdfs:subClassOf :Phenotype .

:Dumpy a owl:Class ;
    rdfs:subClassOf :Phenotype .

:EggLayingDefective a owl:Class ;
    rdfs:subClassOf :Phenotype .

:EmbryonicLethal a owl:Class ;
