@prefix :     <http://www.flymine.org/model/genomic#>.
@prefix rnai: <http://www.flymine.org/model/rnai#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix owl:  <http://www.w3.org/2002/07/owl#>.
@prefix xsd:  <http://www.w3.org/2001/XMLSchema#>.
@prefix null:	 <http://www.flymine.org/model/null#>.

:Gene a owl:Class;
    rdfs:subClassOf :BioEntity.

:Gene__wormBaseSequenceName;
    owl:equivalentProperty rnai:Gene__sequenceName.

:Gene__GCGApprovedName;
    owl:equivalentProperty rnai:Gene__commonName.

:Gene__properties a owl:ObjectProperty;
    owl:equivalentProperty rnai:Gene__phenotypes.

:RNAiExperiment a owl:Class;
    rdfs:subClassOf :Experiment.

null:Phenotype__code a owl:DatatypeProperty;
    owl:equivalentProperty rnai:Phenotype_code.

:Phenotype a owl:Class.

#Mapping from phenotype code -> phenotype ontology class
:WildType a owl:Class;
    rdfs:subClassOf :Phenotype;
    rdfs:subClassOf rnai:Phenotype;
    rdfs:subClassOf
      [ a owl:Restriction;
        owl:onProperty rnai:Phenotype__code;
        owl:hasValue "WT"
      ].