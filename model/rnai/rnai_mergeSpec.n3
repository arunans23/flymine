@prefix :     <http://www.flymine.org/model/genomic#>.
@prefix rnai: <http://www.flymine.org/model/rnai#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix owl:  <http://www.w3.org/2002/07/owl#>.
@prefix xsd:  <http://www.w3.org/2001/XMLSchema#>.
@prefix null:	 <http://www.flymine.org/model/null#> .

:Gene a owl:Class;
    rdfs:subClassOf :BioEntity.

:Gene__properties a owl:ObjectProperty;
    owl:equivalentProperty rnai:Gene__phenotypes.

:RNAiExperiment a owl:Class;
    rdfs:subClassOf :Experiment.

null:Phenotype a owl:Class;
    owl:equivalentClass rnai:Phenotype.

null:WildType__code a owl:DatatypeProperty;
    owl:equivalentProperty :WildType_code.

null:Phenotype__code a owl:DatatypeProperty;
    owl:equivalentProperty rnai:Phenotype_code.

#TODO: remove subClassOf BioProperty, and merge in phenotype ontology
:WildType a owl:Class;
    rdfs:subClassOf :BioProperty;
    rdfs:subClassOf rnai:Phenotype;
    rdfs:subClassOf
      [ a owl:Restriction;
        owl:onProperty rnai:Phenotype__code;
        owl:hasValue "WT"
      ].