# Base: 
@prefix :        <http://www.flymine.org/model/genomic#> .
@prefix mage:       <http://www.flymine.org/model/mage#> .
@prefix null:    <http://www.flymine.org/model/null#> .
@prefix rdfs:    <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf:     <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .
@prefix xsd:     <http://www.w3.org/2001/XMLSchema#> .


# a micro array experiment - can have multiple assays
:MicroArrayExperiment a owl:Class;
    rdfs:subClassOf :Experiment;
    owl:equivalentClass mage:Experiment.

:MicroArrayExpermiment__assays a owl:ObjectProperty;
    rdfs:domain :MicroArrayExperiment;
    rdfs:range :MicroArrayAssay;
    owl:equivalentProperty mage:Experiment__bioAssays.



# an assay between one slide and one or more SampleTissues
# will need a link to some condition information
:MicroArrayAssay a owl:Class;
    rdfs:subClassOf
      [ a owl:Restriction ;
        owl:maxCardinality "1" ;
        owl:onProperty :MicroArrayAssay__slide ] ;
    rdfs:subClassOf
      [ a owl:Restriction ;
        owl:maxCardinality "1" ;
        owl:onProperty :MicroArrayAssay__experiment ] ;
    owl:equivalentClass mage:MeasuredBioAssay.

:MicroArrayAssay__experiment a owl:ObjectProperty;
    owl:inverseOf :MicroArrayExperiment__assays.

:MicroArrayAssay__slide a owl:ObjectProperty;
    rdfs:domain :MicroArrayAssay;
    rdfs:range :MicroArraySlide.

:MicroArrayAssay__results a owl:ObjectProperty;
    rdfs:domain :MicroArrayAssay;
    rdfs:range :MicroArrayExperimentalResult;
    owl:equivalentProperty mage:MeasuredBioAssay__measuredBioAssayData.

:MicroArrayAssay__tissue a owl:ObjectProperty;
    rdfs:domain :MicroArrayAssay;
    rdfs:range :SampleTissue.




# values resulting from a MicroaArrayAssay
:MicroArrayExperimentalResult a owl:Class;
    rdfs:subClassOf :ExperimentalResult;
    rdfs:subClassOf
      [ a owl:Restriction ;
        owl:maxCardinality "1" ;
        owl:onProperty :MicroArrayExperimentalResult__reporter ] ;
    rdfs:subClassOf
      [ a owl:Restriction ;
        owl:maxCardinality "1" ;
        owl:onProperty :MicroArrayExperimentalResult__tissue ] ;
    owl:equivalentClass mage:MeasuredBioAssayData.

:MicroArrayExperimentalResult__assays a owl:ObjectProperty;
    owl:inverseOf :MicroArrayAssay__results.

:MicroArrayExperimentalResult__reporter a owl:ObjectProperty;
    rdfs:domain :MicroArrayExperimentalResult;
    rdfs:range :Reporter.

:MicroArrayExperimentalResult__material a owl:ObjectProperty;
    rdfs:domain :MicroArrayExperimentalResult;
    rdfs:range :BioEntity.

:MicroArrayExperimentalResult__tissue a owl:ObjectProperty;
    rdfs:domain :MicroArrayExperimentalResult;
    rdfs:range :SampleTissue.

:MicroArrayExperimentalResult__type a owl:DatatypeProperty;
    rdfs:domain :MicroArrayExperimentalResult;
    rdfs:range xsd:string.

:MicroArrayExperimentalResult__value a owl:DatatypeProperty;
    rdfs:domain :MicroArrayExperimentalResult;
    rdfs:range xsd:float.

:MicroArrayExperimentalResult__flag a owl:DatatypeProperty;
    rdfs:domain :MicroArrayExperimentalResult;
    rdfs:range xsd:string.




# layout of bioentities on a slide
:MicroArraySlideDesign a owl:Class;
    owl:equivalentClass mage:ArrayDesign.

:MicroArraySlideDesign__slides a owl:ObjectProperty;
    rdfs:domain :MicroArraySlideDesign;
    rdfs:range :MicroArraySlide.

:MicroArraySlideDesign__locations a owl:ObjectProperty;
    rdfs:domain :MicroArraySlideDesign;
    rdfs:range :ReporterLocation.

null:ReporterLocation__version a owl:DatatypeProperty;
    owl:equivalentProperty mage:ReporterLocation__version.

null:ReporterLocation__numberOfFeatures a owl:DatatypeProperty;
    owl:equivalentProperty mage:ArrayDesign__numberOfFeatures.



# an instance of a microarray slide
:MicroArraySlide a owl:Class;
    rdfs:subClassOf
      [ a owl:Restriction ;
        owl:maxCardinality "1" ;
        owl:onProperty :MicroArraySlide__design ] ;
    rdfs:subClassOf
      [ a owl:Restriction ;
        owl:maxCardinality "1" ;
        owl:onProperty :MicroArraySlide__assay ] .

:MicroArraySlide__assay a owl:ObjectProperty;
    owl:inverseOf :MicroArrayAssay__slide.

:MicroArraySlide__design a owl:ObjectProperty;
    owl:inverseOf :MicroArraySlideDesign__slides.




:ReporterLocation a owl:Class;
    rdfs:subClassOf
      [ a owl:Restriction ;
        owl:maxCardinality "1" ;
        owl:onProperty :ReporterLocation__design ] ;
    rdfs:subClassOf
      [ a owl:Restriction ;
        owl:maxCardinality "1" ;
        owl:onProperty :ReporterLocation__reporter ] ;
    owl:equivalentClass mage:FeatureReporterMap.

:ReporterLocation__design a owl:ObjectProperty;
    rdfs:domain :ReporterLocation;
    rdfs:range :MicroArraySlideDesign.

:ReporterLocation__reporter a owl:ObjectProperty;
    rdfs:domain :ReporterLocation;
    rdfs:range :BioEntity.

:ReporterLocation__zoneX a owl:DatatypeProperty;
    rdfs:domain :ReporterLocation;
    rdfs:range xsd:integer.

:ReporterLocation__zoneY a owl:DatatypeProperty;
    rdfs:domain :ReporterLocation;
    rdfs:range xsd:integer.

:ReporterLocation__localX a owl:DatatypeProperty;
    rdfs:domain :ReporterLocation;
    rdfs:range xsd:integer.

:ReporterLocation__localY a owl:DatatypeProperty;
    rdfs:domain :ReporterLocation;
    rdfs:range xsd:integer.



:Reporter a owl:Class;
    rdfs:subClassOf
      [ a owl:Restriction ;
        owl:maxCardinality "1" ;
        owl:onProperty :Reporter__location ] ;
    rdfs:subClassOf
      [ a owl:Restriction ;
        owl:maxCardinality "1" ;
        owl:onProperty :Reporter__material ];
    owl:equivalentClass mage:Reporter.

:Reporter__location a owl:ObjectProperty;
    owl:inversOf :ReporterLocation__reporter.

:Reporter__material a owl:ObjectProperty;
     rdfs:domain :Reporter;
     rdfs:range :BioEntity;
     owl:equivalentProperty mage:Reporter__immobilizedCharacteristics.

:Reporter__type a owl:DatatypeProperty;
     rdfs:domain :Reporter;
     rdfs:range xsd:string.




:SampleTissue a owl:Class;
    rdfs:subClassOf :BioEntity;
    owl:equivalentClass mage:LabeledExtract.

# type of sample material e.g. mRNA, genomic_DNA
:SampleTissue__type a owl:DatatypeProperty;
    rdfs:domain :SampleTissue;
    rdfs:range xsd:string.



# easier just to sort this in MageTranslator?
:BioEntity a owl:Class;
    owl:equivalentClass mage:BioSequence.

:BioEntity__residues a owl:DatatypeProperty;
    owl:equivalentProperty mage:BioSequence__sequence.


null:BioSequence__length a owl:DatatypeProperty;
    owl:equivalentProperty mage:BioSequence__length.

null:BioSequence__isApproximateLength a owl:DatatypeProperty;
    owl:equivalentProperty mage:BioSequence__isApproximateLength.

null:BioSequence__isCircular a owl:DatatypeProperty;
    owl:equivalentProperty mage:BioSequence__isCircular.

null:BioSequence__polymerType a owl:DatatypeProperty;
    owl:equivalentProperty mage:BioSequence__polymerType.

null:BioSequence__type a owl:DatatypeProperty;
