null:BioSequence__sequenceDatabases a owl:DatatypeProperty;
    owl:equivalentClass mage:BioSequence__sequenceDatabases.

null:BioSequence__ontologyEntries a owl:DatatypeProperty;
    owl:equivalentClass mage:BioSequence__ontologyEntries.



:Synonym a owl:Class;
    owl:equivalentClass mage:DatabaseEntry.

:Synonym__name a owl:DatatypeProperty;
    owl:equivalentProperty mage:DatabaseEntry__accession.

:Database__title a owl:DatatypeProperty;
    owl:equivalentProperty mage:Database__name.

:Publication a owl:Class;
    owl:equivalentClass mage:BibliographicReference.

:DAGTerm a owl:Class;
    owl:equivalentClass mage:OntologyEntry.

null:BibliographicReference__authors a owl:DatatypeProperty;
    owl:equivalentProperty mage:BibliographicReference__authors.





