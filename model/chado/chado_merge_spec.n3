:BAC a owl:Class ;
      rdfs:subclassOf :LocatedSequenceFeature ;
      rdfs:subClassOf chado:feature ;
      rdfs:subClassOf
        [ a owl:Restriction ;
           owl:onProperty chado:feature__type ;
           owl:hasValue
               [  rdfs:subClassOf chado:cvterm ;
                  rdfs:subClassOf
                      [ a owl:Restriction ;
                        owl:onProperty chado:cvterm__name ;
                        owl:hasValue "BAC"
                      ] ;
                  rdfs:subClassOf
                      [ a owl:Restriction ;
                        owl:onProperty chado:cvterm__cv ;
                        owl:hasValue
                            [ rdfs:subClassOf chado:cv ;
                              rdfs:subClassOf
                                   [ a owl:Restriction ;
                                       owl:onProperty chado:cv__name ;
                                       owl:hasValue "SO"
                                   ]
                            ]
                      ]
               ]
       ] .

