null:featureprop a owl:Class ;
    owl:equivalentClass chado:featureprop .

# ------------ chado:feature_relationship ------------ #

# feature_relationship maps either to SimpleRelation or RankedRelation
# if subject of relationship is exon -> RankedRelation

:RankedRelation a owl:Class ;
      rdfs:subClassOf chado:feature_relationship ;
      rdfs:subClassOf
        [ a owl:Restriction ;
           owl:onProperty chado:feature_relationship__subject ;
           owl:hasValue
             [ rdfs:subClassOf chado:feature ;
               rdfs:subClassOf 
                 [ a owl:Restriction ;
                   owl:onProperty chado:feature__type ;
                   owl:hasValue
                     [  rdfs:subClassOf chado:cvterm ;
                        rdfs:subClassOf
                          [ a owl:Restriction ;
                            owl:onProperty chado:cvterm__name ;
                            owl:hasValue "exon"
                          ] ;
                        rdfs:subClassOf
                          [ a owl:Restriction ;
                            owl:onProperty chado:cvterm__cv ;
                            owl:hasValue
                               [ rdfs:subClassOf chado:cv ;
                                  rdfs:subClassOf
                                    [ a owl:Restriction ;
                                       owl:onProperty chado:cv__name ;
                                       owl:hasValue "SO"
                                   ]
                               ]
                          ]
                    ]
                ]
            ]
       ] .

null:feature_relationship__rank a owl:DatatypeProperty ;
      owl:equivalentClass chado:feature_relationship__rank .

