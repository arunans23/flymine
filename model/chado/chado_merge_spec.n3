:LocatedSequenceFeature a owl:Class ;
    rdfs:subClassOf :BioEntity ;
    owl:equivalentClass chado:feature .

:LocatedSequenceFeature__synonyms a owl:ObjectProperty ;
    rdfs:domain :LocatedSequenceFeature ;
    rdfs:range :Synonym ;
    rdfs:subPropertyOf :BioEntity__synonyms ;
    owl:equivalentProperty chado:feature__dbxref ;
    owl:equivalentProperty chado:feature__feature_synonyms .

