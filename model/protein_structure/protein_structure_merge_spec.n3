@prefix :        <http://www.flymine.org/model/genomic#> .
@prefix protein_structure:     <http://www.flymine.org/model/protein_structure#> .
@prefix null:    <http://www.flymine.org/model/null#> .
@prefix rdfs:    <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf:     <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .
@prefix xsd:     <http://www.w3.org/2001/XMLSchema#> .

# this merge spec does not specify any equivalences, it merely defines all necessary classes in the target namespace, and nulls all classes in the source namespace.

:Protein a owl:Class;
   rdfs:subClassOf :BioEntity.
   
:Protein__primaryAccession a owl:DatatypeProperty;
   rdfs:domain :Protein;
   rdfs:range xsd:string.

:Protein__regions a owl:ObjectProperty;
   rdfs:domain :Protein;
   rdfs:range :ProteinRegion;
   owl:inverseOf :ProteinRegion__protein.

#####

:ProteinRegion a owl:Class;
   rdfs:subClassOf :BioEntity;
   rdfs:subClassOf [a owl:Restriction;
                    owl:maxCardinality "1";
                    owl:onProperty :ProteinRegion__protein];
   rdfs:subClassOf [a owl:Restriction;
                    owl:maxCardinality "1";
                    owl:onProperty :ProteinRegion__sequenceFamily].
#   rdfs:subClassOf [a owl:Restriction;
#                    owl:maxCardinality "1";
#                    owl:onProperty :ProteinRegion__structuralFamily].
#:ProteinRegion__sequenceFamily a owl:ObjectProperty;
#   rdfs:domain :ProteinRegion;
#   rdfs:range :SequenceFamily.
#
#:ProteinRegion__structuralFamily a owl:ObjectProperty;
#   rdfs:domain :ProteinRegion;
#   rdfs:range :StructuralFamily.
#
#:SequenceFamily a owl:Class.
#
#:SequenceFamily__name a owl:DatatypeProperty;
#   rdfs:domain :SequenceFamily;
#   rdfs:range xsd:string.
#
#:StructuralFamily a owl:Class.
#
#:StructuralFamily__name a owl:DatatypeProperty;
#   rdfs:domain :StructuralFamily;
#   rdfs:range xsd:string.

:ProteinRegion__protein a owl:ObjectProperty;
   rdfs:domain :ProteinRegion;
   rdfs:range :Protein.

:ProteinRegion__structures a owl:ObjectProperty;
   rdfs:domain :ProteinRegion;
   rdfs:range :ProteinStructure;
   owl:inverseOf :ProteinStructure__region.

:ProteinRegion__sequenceFamily a owl:ObjectProperty;
   rdfs:domain :ProteinRegion;
   rdfs:range :ProteinSequenceFamily.

:ProteinSequenceFamily a owl:Class;
   rdfs:subClassOf :BioProperty.

:ProteinSequenceFamily__name a owl:DatatypeProperty;
   rdfs:domain :ProteinSequenceFamily;
   rdfs:range xsd:string.

:ProteinSequenceFamily__regions a owl:ObjectProperty;
   rdfs:domain :ProteinSequenceFamily;
   rdfs:range :ProteinRegion;
   owl:inverseOf :ProteinRegion__sequenceFamily.

:ProteinStructure a owl:Class;
   rdfs:subClassOf :BioProperty;
   rdfs:subClassOf [a owl:Restriction;
                    owl:maxCardinality "1";
                    owl:onProperty :ProteinStructure__region].

:ProteinStructure__region a owl:ObjectPropery;
   rdfs:domain :ProteinStructure;
   rdfs:range :ProteinRegion.

:ProteinStructure__atm a owl:DatatypeProperty;
   rdfs:domain :ProteinStructure;
   rdfs:range xsd:string.

:ModelledProteinStructure a owl:Class;
   rdfs:subClassOf :ProteinStructure.

:ModelledProteinStructure__QScore a owl:DatatypeProperty;
   rdfs:domain :ModelledProteinStructure;
   rdfs:range xsd:decimal.

:ModelledProteinStructure__ZScore a owl:DatatypeProperty;
   rdfs:domain :ModelledProteinStructure;
   rdfs:range xsd:decimal.

#####

null:Fragment_Protein_structure a owl:Class;
   owl:equivalentClass protein_structure:Fragment_Protein_structure.

null:Modelled_region_Fragment_Protein_structure a owl:Class;
   owl:equivalentClass protein_structure:Modelled_region_Fragment_Protein_structure.

null:Alignment_file_Model_Fragment_Protein_structure a owl:Class;
   owl:equivalentClass protein_structure:Alignment_file_Model_Fragment_Protein_structure.

null:Structural_family_Fragment_Protein_structure a owl:Class;
   owl:equivalentClass protein_structure:Structural_family_Fragment_Protein_structure.

null:Protein_structure a owl:Class;
   owl:equivalentClass protein_structure:Protein_structure.

null:Alignment_file_Structural_family_Fragment_Protein_structure a owl:Class;
   owl:equivalentClass protein_structure:Alignment_file_Structural_family_Fragment_Protein_structure.

null:Model_Fragment_Protein_structure a owl:Class;
   owl:equivalentClass protein_structure:Model_Fragment_Protein_structure.

null:Sequence_family_Fragment_Protein_structure a owl:Class;
   owl:equivalentClass protein_structure:Sequence_family_Fragment_Protein_structure.
